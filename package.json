{
  "name": "final-countdown-js",
  "description": "A library of React hooks to manage counters, timers and stopwatches",
  "repository": {
    "type": "git",
    "url": "https://github.com/dlcastillop/final-countdown-js"
  },
  "keywords": [
    "timer",
    "timers",
    "stopwatch",
    "stopwatches",
    "counter",
    "counters"
  ],
  "author": "Daniel Castillo <daniel@dlcastillop.com",
  "license": "MIT",
  "private": false,
  "version": "2.0.1",
  "type": "module",
  "main": "dist/final-countdown-js.umd.js",
  "module": "dist/final-countdown-js.es.js",
  "types": "dist/final-countdown-js.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-dts": "^3.3.1"
  },
  "exports": {
    ".": {
      "import": "./dist/final-countdown-js.es.js",
      "require": "./dist/final-countdown-js.umd.js",
      "types": "./dist/final-countdown-js.d.ts",
      "default": "./dist/final-countdown-js.es.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ]
}
